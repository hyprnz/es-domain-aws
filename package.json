{
  "name": "@hyprnz/es-domain-azure",
  "version": "0.0.0",
  "description": "es-domain implementations for azure services",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/hyprnz/es-domain-azure"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsc -p .",
    "clean": "rm -rf dist/** && rm -rf ./tsconfig.tsbuildinfo",
    "clean:build": "npm run clean && npm run build",
    "test:micro": "jest .micro.ts --passWithNoTests",
    "test:integration": "jest .integration.ts --passWithNoTests --detectOpenHandles --forceExit",
    "test:integration:ci": "jest .integration.ts --passWithNoTests --detectOpenHandles --forceExit",
    "docker:dev": "docker-compose -f ./dev/docker-compose.yml",
    "docker:dev:up": "npm run docker:dev -- up -d",
    "docker:dev:down": "npm run docker:dev -- down -v",
    "docker:dev:connect": "npm run docker:dev -- exec dev bash",
    "docker:dev:awslogs": "npm run docker:dev -- logs aws -f"
  },
  "author": "HYPR Innovation",
  "license": "ISC",
  "peerDependencies": {
    "@hyprnz/es-domain": "^0.2.1",
    "aws-sdk": "~2"
  },
  "devDependencies": {
    "@hyprnz/es-domain": "^0.2.1",
    "@types/aws-sdk": "~2.7.0",
    "@types/jest": "~29.0.0",
    "aws-sdk": "~2.1214.0",
    "husky": "^7.0.4",
    "jest": "~28.1.3",
    "jest-junit": "~13.0.0",
    "mismatched": "^2.9.2",
    "prettier": "^2.4.1",
    "thespian": "^2.10.1",
    "ts-jest": "~28.0.8",
    "ts-node": "~10.9.1",
    "typescript": "~4.8.2"
  }
}
